import "date"

t0 = date.add(d: -1d, to: today())
tOvernightStart = date.add(d: 20h, to: t0)
tOvernightEnd = date.add(d: 28h, to: t0)

from(bucket: "solar")
  |> range(start: tOvernightStart , stop: tOvernightEnd)
  |> filter(fn: (r) => r["_measurement"] == "inverter" and r["_field"] == "batt_level")
  |> min()
